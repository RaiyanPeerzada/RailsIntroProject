<h1>Students#index</h1>
<p>Find me in app/views/students/index.html.erb</p>
<div class="d-inline-block">
    <%= paginate @students, window: 2 %>
</div>
<table class="table">
    <thead class="thead-dark">
      <tr>
        <th>Student id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Degree</th>
      </tr>
    </thead>
    <% @students.each do |student| %>
      <tr>
        <td><%=link_to student.id, student_path(student)%></td>
        <td><%=student.fname%></td>
        <td><%=student.lname%></td>
        <td><%=student.degree%></td>
      </tr>
    <%end%>
  </table>