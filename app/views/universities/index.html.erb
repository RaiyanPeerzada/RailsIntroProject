<h1>Universities#index</h1>
<p>Find me in app/views/universities/index.html.erb</p>
<div class="d-inline-block">
    <%= paginate @universities, window: 2 %>
</div>
<table class="table">
    <thead class="thead-dark">
      <tr>
        <th>University Name</th>
        <th>Country</th>
        <th>Website</th>
        <th>Latitude</th>
        <th>Longitude</th>
      </tr>
    </thead>
    <% @universities.each do |uni| %>
      <tr>
        <td><%=link_to uni.name, university_path(uni)%></td>
        <td><%=uni.country%></td>
        <td><%=uni.webpage%></td>
        <td><%=uni.lat%></td>
        <td><%=uni.long%></td>
      </tr>
    <%end%>
  </table>